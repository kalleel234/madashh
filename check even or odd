import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int i=s.nextInt();
        if(i%2==0){
            System.out.println("Even");
        }
                
                    
            else{
                System.out.println("Odd");
            }
    }
}
