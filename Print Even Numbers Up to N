import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int i;
        int N=s.nextInt();
        for(i=1;i<=N;i++){
          if(i%2==0){
              System.out.print(i+" ");
          }
        }
    }
    
}
